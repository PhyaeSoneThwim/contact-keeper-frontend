{
  "name": "contact-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "autoprefixer": "^9.8.6",
    "axios": "^0.20.0",
    "concurrently": "^5.3.0",
    "formik": "^2.1.5",
    "jwt-decode": "^2.2.0",
    "postcss-cli": "^7.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.11.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-spinners-kit": "^1.9.1",
    "styled-components": "^5.1.1",
    "tailwindcss": "^1.7.3",
    "yup": "^0.29.3"
  },
  "scripts": {
    "start": "concurrently \"yarn start:react\" \"yarn watch:css\"",
    "build": "concurrently \"yarn build:react\" \"yarn build:css\"",
    "watch:css": "postcss tailwind-config.css -o src/assets/style/tailwind.min.css",
    "build:css": "postcss tailwind-config.css -o src/assets/style/tailwind.min.css",
    "start:react": "react-scripts start",
    "build:react": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000"
}
